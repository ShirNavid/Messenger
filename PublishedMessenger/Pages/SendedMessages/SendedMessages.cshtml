@page "/SendedMessages"
@model Massenger.Pages.SendedMessages.SendedMessagesModel

<web-grid grid-id="@Model.Grid.WebComponentId"></web-grid>
<paging-buttons-group grid-id="@Model.Grid.WebComponentId"></paging-buttons-group>

<style>
    html {
        height: 100%;
    }

    body {
        margin: 0px;
        height: 100%;
        background-color: #FFFFFF;
    }

    web-grid {
        width: 100%;
        height: 400px;
        position: relative;
        display: inline-block;
        margin-top: 10px;
    }

    paging-buttons-group {
        position: relative;
        display: inline-block;
        margin-top: 10px;
        left: 50%;
        transform: translateX(-50%);
    }

</style>

<link rel="stylesheet" href="~/css/Components/Scroll.css" />

<script src="~/js/PageFiles/SendedMessagesFunctions.js"></script>
